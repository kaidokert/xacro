<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" xmlns:foo="http://example.com/foo" name="test">
  <!-- These should be processed -->
  <xacro:property name="valid1" value="1.0"/>
  <property name="valid2" value="2.0"/>

  <!-- This should NOT be processed (different namespace) -->
  <foo:property name="ignored" value="999.0"/>

  <!-- Use properties -->
  <link name="test1">
    <box size="${valid1}"/>
  </link>
  <link name="test2">
    <box size="${valid2}"/>
  </link>

  <!-- Keep the foo:property element in output -->
  <foo:property name="ignored" value="999.0"/>
</robot>
